{
  "name": "farther-subsquid",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "migrate": "sqd typegen && sqd codegen && sqd migration:generate && sqd migration:apply",
    "deploy": "sqd deploy .",
    "deploy:reset": "sqd deploy . --hard-reset",
    "setenv": "ts-node ./src/setenv.ts",
    "deploy:staging": "ENVIRONMENT=staging pnpm setenv && pnpm run deploy",
    "deploy:prod": "ENVIRONMENT=production pnpm setenv && pnpm run deploy",
    "sqd:local:prod": "sqd down && ENVIRONMENT=production sqd up && ENVIRONMENT=production sqd process"
  },
  "dependencies": {
    "@subsquid/evm-processor": "^1.18.0",
    "@subsquid/evm-typegen": "^3.3.0",
    "@subsquid/graphql-server": "^4.5.1",
    "@subsquid/typeorm-codegen": "^2.0.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.4.0",
    "@subsquid/util-internal": "^3.2.0",
    "dotenv": "^16.4.5",
    "pg": "^8.11.5",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "viem": "^2.10.1",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "ethers": "^6.11.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  }
}
